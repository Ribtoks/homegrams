					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
						<border>5</border>
						<object class="wxTextCtrl" name="rightBoundTextBox">
							<value>10</value>
							<maxlength>0</maxlength>
